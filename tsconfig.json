{
  "compilerOptions": {
    "baseUrl": "./",
    "paths": {
        "src/*": ["src/*"],
    },
    "lib": [
      "dom",
      "dom.iterable",
      "esnext"
    ],
    "outDir": "./dist/",
    "noImplicitAny": true,
    "experimentalDecorators": true,
    "moduleResolution": "node",
    "module": "es6",
    "target": "es6",
    "jsx": "react",
    "allowJs": true,
    "sourceMap": true,
  }
}